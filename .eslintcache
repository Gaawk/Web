Change={(e) => {\n                  handleChange(e);\n                }}\n              />\n              <span\n                onClick={() => {\n                  setSeePassword({\n                    ...password,\n                    password2: !password.password2,\n                  });\n                }}\n                className=\"material-icons light-text see-password-register position-passwords\"\n              >\n                {password.password2 ? \"visibility\" : \"visibility_off\"}\n              </span>\n              <DontMatch\n                field={\"Password\"}\n                display={error.passwordsDontMatch}\n              />\n            </div>\n            <div className=\"signup-input-row text-center\">\n              <p className=\"form-instructions light-text\">\n                By clicking Sign Up you agree to our Terms, Data policy and\n                Cookie policy. You may recieve emails and SMS notifications from{\" \"}\n                <strong>\n                  g<span style={{ textDecoration: \"underline\" }}>aawk</span>\n                </strong>\n                , but you can opt out at any time.\n              </p>\n            </div>\n\n            <div className=\"form-group submit-button-container\">\n              <button\n                className=\"btn btn-lg submit-button formButton\"\n                type=\"submit\"\n              >\n                NEXT\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </Fragment>\n  );\n};\n\nexport default connect(null, { registerUser })(AuthGender);\n","/Users/emiriana/Desktop/evolution/src/components/ErrorMessages/Required.js",[],"/Users/emiriana/Desktop/evolution/src/components/ErrorMessages/Under18.js",[],"/Users/emiriana/Desktop/evolution/src/components/ErrorMessages/Invalid.js",[],"/Users/emiriana/Desktop/evolution/src/components/ErrorMessages/DontMatch.js",[],"/Users/emiriana/Desktop/evolution/src/components/ProtectedRoutes/P